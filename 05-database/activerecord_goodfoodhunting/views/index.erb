<ul>
  <li><a href="/">all</a></li>
  <% @dish_types.each do |dish_type| %>

    <li>
      <a href="/?id=<%= dish_type.id %>">
        <%= dish_type.name %>
      </a>
    </li>

  <% end %>
</ul>


<h1>hello world</h1>
<a href="/dishes/new">new dish</a>

<% @dishes.each do |dish| %>

  <p>
    <a href="/dishes/<%= dish.id %>">
      <%= dish.name %>
    </a>
  </p>
  
  <a href="/dishes/<%= dish.id %>">
    <img src="<%= dish.image_url %>" alt="">
  </a>

<% end %>